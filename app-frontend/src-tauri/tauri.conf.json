{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "scooterlease",
  "version": "0.1.0",
  "identifier": "com.scooterlease.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "scooterlease",
        "width": 430,
        "height": 932
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.ico"
    ],
    "android": {
      "minSdkVersion": 21
    }
  }
}